   "source": [
    "#연습문제 7.5\n",
    "\n",
    "char = input('한 문자 입력? ')\n",
    "\n",
    "def find_char_type(c):\n",
    "    if c.isdigit():\n",
    "        return '숫자'\n",
    "    elif c.isupper() or c.islower():\n",
    "        return '알파벳'\n",
    "    elif c.isspace():\n",
    "        return '공백'\n",
    "    else:\n",
    "        return '기타'\n",
    "    \n",
    "\n",
    "\n",
    "print(f'{find_char_type(char)} 문자를 입력했습니다.')"
   ]
  }
